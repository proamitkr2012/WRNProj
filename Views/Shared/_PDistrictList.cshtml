@model List<tblDistrict>


@if (Model != null)
{
    if (Model.Count > 0)
    {
        <div>

            <select class="form-control" id="PDistrict" name="PDistrict">
                <option value="">Choose District </option>
                @if (Model != null)
                {
                    foreach (var item in Model)
                    {
                        <option value="@item.Name">@item.Name</option>

                    }
                }


            </select>

        </div>
    }
    else
    {
        <input type="text" placeholder="" class="form-control" id="PDistrict" name="PDistrict" >
    }
}

