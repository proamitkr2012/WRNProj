                                    <div class="col-md-2"></div>
                                    <div class="col-md-4"><a href="javascript:void(0)" onclick="return DocumentPrint('')" class="btn btn-primary btn-block text-white"> Print</a> </div>
                                    <div class="col-md-4"></div>
                                </div>

                            </div>
                        </div>
                    </div>

                }
                    </div>
                    </div>
                    </div>
                
</form>
                <script language="javascript" type="text/javascript">

                    function windowclose() {
                        window.close();


                    }

                    function DocumentPrint(name) {

                        var display_settings = "toolbar=yes,location=no,directoried=yes,menubar=yes,scrollbars=yes,top=25,left=100,height=500 width=800,resizable=yes";
                        var content = document.getElementById("print1").innerHTML;
                        var docprint = window.open('', '', display_settings);
                        docprint.document.write('<html><head></head><form onLoad="self.print();"><center>' + content + '</center></form></html>');
                        docprint.print();
                        docprint.document.close();
                        docprint.focus();
                        return false;
                    }


                </script>


 