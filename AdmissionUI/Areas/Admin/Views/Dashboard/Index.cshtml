<div class="row pb-10">

    <div class="col-md-6 mb-20">
        <div class="card-box min-height-200px pd-20 mb-20"
             data-bgcolor="orange">
            <div class="d-flex justify-content-between pb-20 text-white">
                <div class="icon h1 text-white">
                    <i class="fa fa-users" aria-hidden="true"></i>

                </div>
                <div class="font-14 text-right">

                    <div class="font-30">WRN Registrations</div>
                </div>
            </div>
            <div class="d-flex justify-content-between align-items-end">
                <div class="text-white">
                    <div class="font-14">Total Application(Candidate) </div>
                    <div class="font-24 weight-500">
						<a asp-controller="NewStudent" asp-action="TotalStudentDetails" target="_blank" class="text-decoration-none text-white">	@Model.NewAdmissionDataCount.TotalWRN</a>

					</div>
                </div>
                <div class="text-white">
					<div class="font-14">Fee Paid (Candidate)</div>
					<div class="font-24 weight-500">
						<a asp-controller="NewStudent" asp-action="RegStudentDetails" target="_blank" class="text-decoration-none text-white">
						@Model.NewAdmissionDataCount.TotalRegistered
						</a>
					</div>
                </div>
            </div>
        </div>

    </div>
	<div class="col-md-6 mb-20">
		<div class="card-box min-height-200px pd-20 mb-20"
			 data-bgcolor="white">
			<div class="d-flex justify-content-between pb-20 text-white">
				<div class="icon h1 text-black">
					<i class="fa fa-money" aria-hidden="true"></i>

				</div>
				<div class="font-14 text-right ">

					<div class="font-30 text-black">Course Fee</div>
				</div>
			</div>
			<div class="d-flex justify-content-between align-items-end">
				<div class="text-black">
					<div class="font-14">Fees Paid </div>
					<div class="font-24 weight-500">
						<a asp-controller="NewStudent" asp-action="StudentsCourseFees" target="_blank" class="text-decoration-none">
						@Model.NewAdmissionDataCount.NoofFeePaid
						</a>
					</div>
				</div>
				<div class="text-black">
					<div class="font-14">Total Amount</div>
					<div class="font-24 weight-500">
						<a asp-controller="NewStudent" asp-action="StudentsFees" target="_blank" class="text-decoration-none">
						@Model.NewAdmissionDataCount.TotalAmount.ToString("#0,00")
						</a>
					</div>
				</div>
			</div>
		</div>

	</div>
	<div class="col-md-6 mb-20">
		<div class="card-box min-height-200px pd-20 mb-20"
			 data-bgcolor="green">
			<div class="d-flex justify-content-between pb-20 text-white">
				<div class="icon h1 text-white">
					<i class="fa fa-users" aria-hidden="true"></i>

				</div>
				<div class="font-14 text-right">

					<div class="font-30">B.Ed Registrations</div>
				</div>
			</div>
			<div class="d-flex justify-content-between align-items-end">
				<div class="text-white">
					<div class="font-14">Total Application(Candidate)</div>
					<div class="font-24 weight-500">

						<a asp-controller="Dashboard" asp-action="TotalBedStudentDetails" target="_blank" class="text-decoration-none text-white">
							@Model.BedAdmissionDataCount.TotalWRN
