<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Dr Bhimrao Ambedkar University</title>
 
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css" integrity="sha512-iecdLmaskl7CVkqkXNQ/ZH/XLlvWZOJyj7Yy7tcenmpD1ypASozpmT/E0iPtmFIB46ZmdtAc9eNBvH0H/ZpiBw==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon">
     <link rel="stylesheet" type="text/css" href="/Themes/vendors/styles/core.css" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link href="~/css/bootstrap-steps.min.css" rel="stylesheet" />
    <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script><style>
        .input-validation-error {
            background: #fa00001c;
            border: 1px solid #ff0000 !important;
        }
        .card-body{
            background: rgba(0,0,0,.1);
        }
    </style>
</head>
<body>
    @await Html.PartialAsync("_header")
    <div class="">
        <div class="">
            <main role="main" id="wrap">
                @RenderBody()
            </main>
        </div>
    </div>
    @await Html.PartialAsync("_footer")
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"></script>
    <!-- js -->
    <script src="/Themes/vendors/scripts/core.js"></script>
    <script src="/Themes/vendors/scripts/script.min.js"></script>
    <script src="/Themes/vendors/scripts/process.js"></script>
    <script src="/Themes/vendors/scripts/layout-settings.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
