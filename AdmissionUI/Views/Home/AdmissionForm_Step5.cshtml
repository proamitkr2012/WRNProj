@model StudentMastersDTO
@{
    ViewData["Title"] = "AdmissionForm";

}
<style>
    td, th {
        padding: 3px 5px !important;
    }

    th {
        border-right: 1px solid #000 !important;
    }


    body {
        margin: 0;
        padding: 0;
        background-color: #FAFAFA;
        font: 12pt "Tahoma";
    }

    * {
        box-sizing: border-box;
        -moz-box-sizing: border-box;
    }

